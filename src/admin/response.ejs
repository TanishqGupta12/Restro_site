<!DOCTYPE html>
<html>
<head>
  <title>Contacts</title>
  <style>
    td:nth-child(even),
    th:nth-child(even) {
      background-color: #D6EEEE;
    }

    h1 {
      text-align: center;
    }
    table {
      width: 800px;
    }
    table,
    th,
    td {
      border: 1px solid black;
    }
  </style>
</head>

<body>
  <div class="nav">
    <a href="from.ejs">Reservations</a> |
    <a href="response.ejs">Contacts</a> |
  </div>
  <h1>Contacts</h1>
  <table>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Message</th>
    </tr>
    <%data.forEach((item) => {%>
    <tr>
      <td><%= item.name%></td>
      <td><%= item.email%></td>
      <td><%= item.message%></td>
    </tr>
    <% }) ;%>
  </table>
  <!-- <ul>
    <% data.forEach((item) => { %>
      <li><%= item.name %> - <%= item.email %> - <%= item.message%> </li>
    <% }); %>
  </ul> -->
</body>
</html>