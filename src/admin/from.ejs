    <% data.forEach((item) => { %>
      <li><%= item.dates %> - <%= item.times %> - <%= item.Person_number%>  - <%= item.Person_name%> - <%= item.Person_email%> - <%= item.Phone_number%></li>
    <% }); %>
  </ul> -->
</body>
</html>