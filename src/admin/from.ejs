<!DOCTYPE html>
<html>
<head>
  <title>Reservations</title>
  <style>
    td:nth-child(even),
    th:nth-child(even) {
      background-color: #D6EEEE;
    }

    h1 {
      text-align: center;
    }
    table {
      width: 800px;
    }

    table,
    th,
    td {
      border: 1px solid black;
    }
  </style>
</head>
<body>
  <div class="nav">
    <a href="from.ejs">Reservations</a> |
    <a href="response.ejs">Contacts</a> |
  </div>
  <h1>Reservations</h1>
  <table>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Person_number</th>
      <th>Person_name</th>
      <th>Person_email</th>
      <th>Phone_number</th>
    </tr>
    <% data.forEach((item) => { %>
    <tr>
      <td><%= item.dates %></td>
      <td><%= item.times %></td>
      <td><%= item.Person_number%></td>
      <td><%= item.Person_name%></td>
      <td><%= item.Person_email%></td>
      <td><%= item.Phone_number%></td>
    </tr>
    <% }); %>

 </table>



